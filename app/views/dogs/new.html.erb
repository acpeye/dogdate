Dog Create Page

<%= simple_form_for [@owner, @dog] do |f|%>
<%= f.input :name %>
<%= f.input :description %>
<%= f.input :breed %>
<%= f.input :images_url  %>
<%= f.submit "Preview", formaction: owner_preview_dog_path %>
<% unless @dog.images_url.empty? %>
  <div id="rounded-border">
    <%= image_tag @dog.images_url[0], width: 300 %>
  <div class="card-content">
<% end %>
<%= f.input :available_dates %>
<%= f.submit %>
<% end %>
